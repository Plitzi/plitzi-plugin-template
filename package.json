{
  "name": "@plitzi/plitzi-plugin-template",
  "version": "0.6.0",
  "homepage": "./",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "type": "module",
  "repository": {
    "type": "https",
    "url": "https://github.com/plitzi/plitzi-plugin-template.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/plitzi"
  },
  "main": "dist/plitzi-plugin-demo.mjs",
  "module": "dist/plitzi-plugin-demo.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.scss"
  ],
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "peerDependencies": {
    "@plitzi/plitzi-sdk": "^0.24.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@plitzi/plitzi-sdk": "^0.27.1",
    "@rollup/plugin-replace": "^6.0.3",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-links": "^10.1.8",
    "@storybook/addon-styling-webpack": "^3.0.0",
    "@storybook/react": "^10.1.8",
    "@storybook/react-vite": "^10.1.8",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.21",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@vitejs/plugin-react": "^5.1.2",
    "cssnano": "^7.1.2",
    "eslint": "9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.8",
    "flow-bin": "^0.294.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.96.0",
    "storybook": "^10.1.8",
    "stylelint": "^16.26.1",
    "stylelint-order": "^7.0.0",
    "stylelint-scss": "^6.13.0",
    "tailwindcss": "4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-plugin-zip-file": "^4.1.1",
    "vitest": "^4.0.15",
    "vitest-fetch-mock": "^0.4.5"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "dev": "vite preview --port 5000",
    "build:dev": "tsc -p ./tsconfig.app.json && vite build --mode development && node ./scripts/generate-exports.mjs",
    "build:dev-gzip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true vite build --mode development && node ./scripts/generate-exports.mjs",
    "build:dev-zip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true AS_ZIP=true vite build --mode development",
    "build:dev-watch": "tsc -p ./tsconfig.app.json && vite build --mode development --watch",
    "build:prod": "tsc -p ./tsconfig.app.json && vite build",
    "build:prod-gzip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true vite build",
    "build:prod-zip": "tsc -p ./tsconfig.app.json && ONLY_GZIP=true AS_ZIP=true vite build",
    "build:analyze": "npx vite-bundle-visualizer",
    "typecheck": "tsc --noEmit",
    "lint": "TIMING=1 eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint-ts": "tsc",
    "lint-style": "stylelint \"**/*.scss\" --custom-syntax postcss-scss",
    "lint-style:fix": "stylelint \"**/*.scss\" --custom-syntax postcss-scss --fix",
    "build-storybook": "build-storybook"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.2.2"
}
